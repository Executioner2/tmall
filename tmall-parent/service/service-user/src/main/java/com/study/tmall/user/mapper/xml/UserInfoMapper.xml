<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.tmall.user.mapper.UserInfoMapper">
    <update id="setOpenidById">
        UPDATE
            user_info
        SET
            openid=#{openid}
        WHERE
            id=#{id}
    </update>
</mapper>